<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherited_view_partner_form_owner" model="ir.ui.view">
        <field name="name">inherited_view_partner_form_owner</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/group/group/field[@name='category_id']" position="after">
                <field name="owner_id" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>